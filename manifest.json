{  
   "name":"US-Arabic Transliteration Keyboard",
   "description":"Keyboard that lets you type with Arabic Transliteration. For quick words, use Alt+Shift+X, and copy what you type!",
   "version":"1.0",
   "manifest_version":2,
   "background":{  
      "scripts":["jquery-3.1.0.min.js", "background.js"],
      "persistent":false
   },
   "permissions":["tabs", "contextMenus"],
   "browser_action":{  
      "default_popup":"browser_action.html"
   },
   "content_scripts":[  
      {  
         "matches":["<all_urls>"],
         "js":["jquery-3.1.0.min.js", "content.js"],
         "run_at":"document_end"
      }
   ],
   "commands":{
      "_execute_browser_action": {
         "suggested_key": {
          "default": "Alt+Shift+X",
          "mac": "Alt+Shift+X"
       },
       "description":"Popup Shortcut."
       }
   }
}
